# Azure AI Configuration
AZURE_AI_CREDENTIAL=your_azure_ai_credential_here
AZURE_AI_ENDPOINT=https://your-resource.services.ai.azure.com/models/

# OpenRouter API
OPENROUTER_API_KEY=sk-or-v1-your_openrouter_api_key_here

# Notion Integration
NOTION_TOKEN=ntn_your_notion_token_here
DATA_SOURCE_ID=your_notion_database_id_here
NOTION_VERSION=2025-09-03

# Project Paths
PROJECT_ROOT=/path/to/rag-report-generator
DATA_DIR=${PROJECT_ROOT}/data
QDRANT_DATA_DIR=${DATA_DIR}/qdrant_data
PROMPTS_BASE_DIR=${PROJECT_ROOT}/app/prompts

# Langfuse Observability
LANGFUSE_SECRET_KEY=sk-lf-your_langfuse_secret_key_here
LANGFUSE_PUBLIC_KEY=pk-lf-your_langfuse_public_key_here
LANGFUSE_HOST=https://cloud.langfuse.com

# Upstage API
UPSTAGE_API_KEY=up_your_upstage_api_key_here

# Embedding Model Preset Selection
# Available values: upstage, openai-large, qwen, bge-m3, gemini
MODEL_PRESET=upstage
